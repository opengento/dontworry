<!-- acl for simple google shopping. Visible in the acl treeview of user group rights -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Opengento_Dontworry::global" title="Opengento - Don't Worry" sortOrder="10">
                    <resource id="Opengento_Dontworry::config" title="Module Configuration" sortOrder="10"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>